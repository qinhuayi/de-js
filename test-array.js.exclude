
Array.prototype.forEach = function (fn) {
    if (typeof (fn) == "function") {
        for (var i = 0; i < this.length; i++) {
            fn(this[i], i);
        }
    }
}
Array.prototype.remove = function (i) {
    return this.splice(i,1) | 1;
};

function Main() {
    var i = 3;
    var arr = [1, 2, 3, 4, 5];

    arr.remove(2);
    return arr.toString();
}
